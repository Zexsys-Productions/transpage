{
  "manifest_version": 3,
  "name": "Transpage",
  "version": "1.0",
  "description": "Dev Version",
  "action": {
    "default_title": "Open Transpage"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
